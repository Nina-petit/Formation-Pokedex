<%- include('partials/header') %>

        <div class="cardsContainer">
            <% pokemons.forEach((pokemon) => { %>
                <article class="card">
                    <a href="/card/<%=pokemon.numero%>">
                        <img src="/img/<%=pokemon.numero%>.png" alt="Illustration de <%=pokemon.nom%>">
                        <h2>#<%=pokemon.numero%> <%=pokemon.nom%></h2>
                    </a>
                </article>
            <% }) %>
        </div>
    </main>
</body>
</html>